{
 "compilerOptions": {
  "sourceMap": true,
  "noImplicitReturns": true,
  "module": "commonjs",
  "moduleResolution": "node",
  "target": "ESNext",
  "lib": [
   "es2015",
   "es2019",
   "dom"
  ],
  "typeRoots": [
   "./typings",
   "node_modules/@types"
  ]
 },
 "exclude": [
  "./node_modules",
  "./test",
  "./dist",
  "./docs",
 ],
 "include": [
  "src/**/*.ts",
  "src/index.js"
, "src/test/test.ts" ]
}